<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <!--Olá-->
    <category>
	    <pattern>
	        Olá, eu me chamo *
	    </pattern>
	    <template>
	        <think><set name = "nomepessoa"><star /></set></think>
	        <random>
	            <li>Olá <formal><get name="nomepessoa" /></formal>, tudo bem com você?</li>
	            <li>Opa <formal><get name="nomepessoa" /></formal>, como você está?</li>
	            <li>Aoba <formal><get name="nomepessoa" /></formal>, tudo bem?</li>
	        </random>
	    </template>
	</category>
</aiml>